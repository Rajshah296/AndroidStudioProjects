<resources>
    <!-- Base application theme. -->
    <style name="Base.Theme.RentEasy" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">#9F47E1</item>
        <item name="fontFamily">@font/acme</item>
    </style>
    <style name="editTxt_16." parent="Widget.AppCompat.EditText"  >
        <item name="android:layout_marginHorizontal">@dimen/_20sdp</item>
        <item name="android:layout_marginTop">@dimen/_50sdp</item>
        <item name="android:textSize">@dimen/_16sdp</item>
        <item name="android:background">@drawable/custom_border</item>
        <item name="android:paddingHorizontal">@dimen/_20sdp</item>
        <item name="android:paddingVertical">@dimen/_10sdp</item>
        <item name="android:drawablePadding">@dimen/_10sdp</item>
        <item name="android:autofillHints">true</item>
        <item name="android:minHeight">@dimen/_48sdp</item>
        <item name="android:textColorHint">@color/white</item>
    </style>

    <style name="cb_style_14" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:buttonTint">@color/white_trans</item>
        <item name="android:text">@string/remember</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_14sdp</item>
    </style>

    <style name="btn_style_16_1" parent="Widget.AppCompat.Button">


        <item name="android:layout_marginHorizontal">@dimen/_20sdp</item>
        <item name="android:layout_marginTop">@dimen/_20sdp</item>
        <item name="backgroundTint">@null</item>
        <item name="android:background">@drawable/custom_btn</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/_16sdp</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="btn_style_16_2" parent="Widget.AppCompat.Button">


        <item name="android:layout_marginHorizontal">@dimen/_20sdp</item>
        <item name="android:layout_marginTop">@dimen/_20sdp</item>
        <item name="backgroundTint">@null</item>
        <item name="android:background">@drawable/custom_btn_red</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/_16sdp</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="txtView_Style_Login_UI" parent="Widget.AppCompat.TextView">
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:textSize="@dimen/_14sdp"

        <item name="android:layout_marginStart">@dimen/_20sdp</item>
        <item name="android:layout_marginTop">@dimen/_20sdp</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="Theme.RentEasy" parent="Base.Theme.RentEasy" />
</resources>